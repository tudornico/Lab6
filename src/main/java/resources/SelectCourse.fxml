<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="UI.SelectCourseController"
          prefHeight="400.0" prefWidth="600.0">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <Label GridPane.rowIndex="1" GridPane.columnIndex="1" text="Select Course Id"/>
    <TextField fx:id="SelectedCourse" GridPane.columnIndex="2" GridPane.rowIndex="1"/>

    <Button fx:id = "Submit" text="Submit Course" GridPane.columnIndex="5" GridPane.rowIndex="5" onAction="#"/>
</GridPane>
