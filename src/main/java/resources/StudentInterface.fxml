<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="UI.StudentInterfaceController"
          prefHeight="400.0" prefWidth="600.0">

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <Button fx:id="Register" GridPane.columnIndex="1" GridPane.rowIndex="5" onAction="#registerToCourse"/>

    <Button fx:id="All Students" GridPane.columnIndex="8" GridPane.rowIndex="5" onAction="#AllStudentsFromCourse"/>
</GridPane>
